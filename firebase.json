{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
      "source": "**/*.@(jpg|jpeg|gif|png|js|css)",
      "headers":[{
        "key": "Cache-Control",
        "value": "no-cache" 
      }] 
    }] 
  } 
}
